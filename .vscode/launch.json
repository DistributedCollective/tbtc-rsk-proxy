    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "eth",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/eth.js",
            "env": {
                "TARGET_URL": "wss://testnet.sovryn.app/ws",
                "TARGET_PORT": "443",
                "PROXY_PORT": "5050",
                "HTTP_MODE": "0",
                "MATCH_REQUESTS": "0",
                "MUTE_LOGGING": "0",
            }
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "btc",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/btc.js"
        }
    ]
}